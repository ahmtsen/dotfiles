[init]
	defaultBranch = main

[user]
	name = Ahmet Sen
	email = ahmetsen105@gmail.com

[merge]
    conflictstyle = diff3

[diff]
    colorMoved = default

[core]
    pager = delta

[interactive]
    diffFilter = delta --color-only --features=interactive

[include]
	path = ~/.config/delta/catppuccin.gitconfig

[delta]
    features = catppuccin-mocha
